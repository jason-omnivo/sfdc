<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Net_Funded__c</fullName>
    <description>This formula field takes the Loan Amount and deducts the PSF Fee, both Origination Fees and the ACH Program Fee</description>
    <externalId>false</externalId>
    <formula>Loan_Amount__c  -  PSF_Fee__c  -  Origination_Fee_Lender__c -  Origination_Fee_Calc__c  - ACH_Program_Fee__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Net Funded</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
